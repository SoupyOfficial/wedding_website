{
  "name": "wedding_website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "seed": "npx tsx prisma/seed.ts",
    "db:push": "npx prisma db push",
    "db:migrate": "DATABASE_URL=\"file:./prisma/dev.db\" npx prisma migrate dev",
    "db:check": "DATABASE_URL=\"file:./prisma/dev.db\" npx tsx scripts/check-migrations.ts",
    "hooks:install": "cp scripts/git-hooks/pre-push .git/hooks/pre-push && chmod +x .git/hooks/pre-push",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@libsql/client": "^0.17.0",
    "@prisma/adapter-libsql": "^6.19.2",
    "@prisma/client": "^6.19.2",
    "@vercel/analytics": "^1.6.1",
    "autoprefixer": "^10.4.24",
    "bcryptjs": "^3.0.3",
    "cloudinary": "^2.9.0",
    "clsx": "^2.1.1",
    "framer-motion": "^12.34.0",
    "jsonwebtoken": "^9.0.3",
    "next": "^14.2.35",
    "next-auth": "^5.0.0-beta.30",
    "postcss": "^8.5.6",
    "prisma": "^6.19.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.19",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/react": "^18.3.28",
    "@types/react-dom": "^18.3.7",
    "@types/sharp": "^0.31.1",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.35",
    "sharp": "^0.34.5",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}
